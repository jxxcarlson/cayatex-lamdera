module View.Color exposing (blue, darkRed, paleBlue, paleViolet)

import Element as E


darkRed : E.Color
darkRed =
    E.rgb255 140 0 0


blue : E.Color
blue =
    E.rgb255 0 0 200


paleBlue : E.Color
paleBlue =
    E.rgb255 180 180 255


paleViolet : E.Color
paleViolet =
    E.rgb255 230 230 255
